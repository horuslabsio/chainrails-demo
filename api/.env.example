# Your Chainrails API key (starts with cr_test_ for test mode or cr_live_ for live mode)
CHAINRAILS_API_KEY=cr_test_your_key_here

# Chainrails API base URL
CHAINRAILS_API_URL=https://api.chainrails.io/api/v1

# Webhook secret for validating incoming webhooks
CHAINRAILS_WEBHOOK_SECRET=your_webhook_secret_here